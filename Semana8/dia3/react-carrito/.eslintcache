\r\n          {\r\n            ...objProducto,\r\n            cant: 1\r\n          }\r\n        ]\r\n      }\r\n    })\r\n  }\r\n\r\n  return (\r\n    <div className=\"col-xl-3 col-lg-4 col-md-4 col-sm-6 mb-3\">\r\n      <div className=\"card shadow h-100\">\r\n        <img src={objProducto.prod_img} alt=\"\" className=\"img-card-top\" />\r\n        <div className=\"card-body\">\r\n          <p className=\"card-title\">{objProducto.prod_nom}</p>\r\n          <p className=\"card-text\">\r\n            <small className=\"text-muted\">Precio:</small><br />\r\n            <strong>S/ {objProducto.prod_pre}</strong>\r\n          </p>\r\n        </div>\r\n        <div className=\"card-footer text-right\">\r\n          <button className=\"btn btn-outline-dark\" onClick={agregarAlCarrito}>\r\n            <i className=\"fas fa-shopping-cart\"></i>\r\n          </button>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  )\r\n}\r\n\r\nexport default Producto\r\n","D:\\CodiGo\\codigo-grupo5\\Semana8\\dia3\\react-carrito\\src\\components\\filtro\\Filtro.js",[],"D:\\CodiGo\\codigo-grupo5\\Semana8\\dia3\\react-carrito\\src\\components\\carrito\\Carrito.js",[],"D:\\CodiGo\\codigo-grupo5\\Semana8\\dia3\\react-carrito\\src\\components\\carrito\\CarritoItem.js",["42"],"import React from 'react'\r\n\r\nconst CarritoItem = ({ objProducto, setCarrito }) => {\r\n\r\n  const eliminarItem = () => {\r\n    setCarrito((prevState) => {\r\n      let carritoNuevo = prevState.filter((producto) => {\r\n        if (producto.prod_id !== objProducto.prod_id) {\r\n          return producto;\r\n        }\r\n      });\r\n      return carritoNuevo;\r\n    });\r\n  }\r\n\r\n  return (\r\n    <div className=\"carrito-item\">\r\n      <img src={objProducto.prod_img} alt=\"\"\r\n        width=\"25\" />\r\n      <small>{objProducto.prod_nom}</small>\r\n      <small>S/ {objProducto.prod_pre * objProducto.cant}</small>\r\n      <small>({objProducto.cant})</small>\r\n      <button className=\"btn btn-sm btn-ou